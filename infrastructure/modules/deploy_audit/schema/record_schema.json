{
  "title": "NHS PARS Schema",
  "description": "The Patient Audit Record Service's AWS Glue table schema",
  "type": "object",
  "properties": {
    "action": {
      "description": "The type of action that has been performed during the event",
      "example": "R",
      "type": "string"
    },
    "recorded": {
      "description": "The timestamp for the particular AuditEvent",
      "example": "2024-07-01T09:49:00+00:00",
      "type": "timestamp"
    },
    "entity_person_what_identifier_value": {
      "description": "NHS Number",
      "example": "9999999991",
      "type": "string"
    },
    "entity_xrequestid_what_identifier_value": {
      "description": "Request ID",
      "example": "123148b6-586d-11ec-bf63-0242ac130005",
      "type": "string"
    },
    "entity_system_what_identifier_value": {
      "description": "An identifier",
      "example": "123445",
      "type": "string"
    },
    "entity_system_detail_interactionidentifier_valuestring": {
      "description": "Details of the identifier",
      "example": "Health Checks",
      "type": "string"
    },
    "entity_system_detail_productidentifier_valuestring": {
      "description": "Details of the identifier",
      "example": "Health Checks - Get Lab Result",
      "type": "string"
    },
    "entity_system_name": {
      "description": "Full name of the system that has been made the request",
      "example": "Patient Data Manager",
      "type": "string"
    },
    "agent_requestor_who_display": {
      "description": "Identifies who is making the request",
      "example": "A GP Surgery",
      "type": "string"
    },
    "agent_requestor_who_type": {
      "description": "Identifies the type of agent",
      "example": "Organization",
      "type": "string"
    },
    "agent_requestor_who_identifier_value": {
      "description": "An agent identifier value",
      "example": "ABC",
      "type": "string"
    },
    "source_observer_display": {
      "description": "Name of the system that has produced the AuditEvent",
      "example": "Patient Data Manager",
      "type": "string"
    },
    "source_observer_identifier_value": {
      "description": "Where has the original AuditEvent come from",
      "example": "PDM",
      "type": "string"
    },
    "_data": {
      "description": "Original AuditEvent JSON",
      "example": "",
      "type": "string"
    }
  }
}
