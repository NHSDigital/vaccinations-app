{
  "name": "vaccinations-app",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "next dev --turbopack --experimental-https",
    "app": "npm run build && npm run start",
    "build": "next build",
    "build:opennext": "npx --yes @opennextjs/aws@3.x.x build",
    "build:lambda": "node esbuild.config.mjs",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier . --write",
    "check": "npx prettier . --check",
    "test": "tsc --noEmit && jest --collectCoverage",
    "test:watch": "jest --watch",
    "e2e": "npx playwright test e2e/general/",
    "e2e:ui": "npx playwright test e2e/general/ --ui",
    "e2e:snapshot": "npx playwright test e2e/snapshot/",
    "e2e:snapshot-update": "npx playwright test e2e/snapshot/ --update-snapshots",
    "contract": "jest --config=contract/contract.jest.config.ts",
    "elid": "wiremock --root-dir ./wiremock --port 8081 --verbose --local-response-templating"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NHSDigital/vaccinations-app.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NHSDigital/vaccinations-app/issues"
  },
  "homepage": "https://github.com/NHSDigital/vaccinations-app#readme",
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3.3.1",
    "@estruyf/github-actions-reporter": "^1.10.0",
    "@pact-foundation/pact": "^15.0.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/hast": "^3.0.4",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.6",
    "esbuild": "0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-pact": "^0.11.4",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wiremock": "^3.13.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/client-ssm": "^3.849.0",
    "@types/aws-lambda": "^8.10.152",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "15.4.4",
    "next-auth": "^5.0.0-beta.29",
    "nhsuk-react-components": "^5.0.0",
    "openid-client": "^6.6.2",
    "pino": "^9.7.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-markdown": "^10.1.0"
  },
