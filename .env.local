# AWS parameter store
SSM_PREFIX=/local/

# logging
PINO_LOG_LEVEL=info

# content cache hydrator
CONTENT_API_ENDPOINT=http://localhost:8081/
CONTENT_API_KEY=should-not-be-checked-in
CONTENT_CACHE_PATH=wiremock/__files/

# NHS Login auth provider
