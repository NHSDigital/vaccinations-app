NHS_LOGIN_CLIENT_ID=vita-app-sandpit
NHS_LOGIN_SCOPE=openid profile gp_registration_details
NHS_LOGIN_PRIVATE_KEY=should-not-be-checked-in

# Implicit Auth.js variables
AUTH_URL=http://localhost:3000
AUTH_SECRET=should-not-be-checked-in
